{
  "name": "node-graphql-structure",
  "version": "0.0.1",
  "main": "server.ts",
  "author": "Danny Fran√ßa <contato@dannyfranca.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "studio": "dotenv -e .env.production -- prisma studio",
    "studio-dev": "dotenv -e .env.development -- prisma studio",
    "migrate-save": "dotenv -e .env.production prisma migrate save --experimental",
    "migrate-save-dev": "dotenv -e .env.development -- prisma migrate save --experimental",
    "migrate-up": "dotenv -e .env.production prisma migrate up --experimental --verbose",
    "migrate-up-dev": "dotenv -e .env.development -- prisma migrate up --experimental --verbose",
    "test": "cross-env NODE_ENV=production jest",
    "test-dev": "cross-env NODE_ENV=development jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cron": "^1.7.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.160",
    "@types/node": "^14.6.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typegraphql-prisma": "^0.7.0",
    "typescript": "^3.9.5",
    "typescript-mix": "^3.1.3",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "@prisma/client": "2.7.1",
    "apollo-server-express": "^2.17.0",
    "bcrypt": "^5.0.0",
    "bullmq": "^1.9.0",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "nanoevents": "^5.1.8",
    "nodemailer": "^6.4.11",

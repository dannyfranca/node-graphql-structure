{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "camelcase": "off",
        "no-unused-vars": "off",
        "no-useless-catch": "off",
        "no-useless-constructor": "off",
        "standard/no-callback-literal": "off"
    }
}
